    <h1>wOOp! <span>beta</span></h1>
	</header>
	<div id="main" role="main">
    
    {% if message %}
    <div class="{{ message.type }}">
      {{ message.body }}
    {% if message.fields %}
      {% set form_error_fields = message.fields %}
      <ul>
      {% for message in message.fields %}
          <li>{{ message }}</li>
      {% endfor %}
      </ul>
    {% endif %}
    </div>
    {% endif %}
    
    {% block content %}
    {% endblock %}
	</div>
	<footer>
